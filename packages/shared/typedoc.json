{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": ["./lib/index.ts"],
  "out": "./docs/api",
  "plugin": ["typedoc-plugin-markdown", "typedoc-vitepress-theme"],
  "githubPages": false,
  "readme": "none",
  "indexFormat": "table",
  "useCodeBlocks": true,
  "disableSources": true,
  "textContentMappings": {
    "title.indexPage": "API Reference",
    "title.memberPage": "{name}"
  },
  "navigation": {
    "includeCategories": true
  }
}
